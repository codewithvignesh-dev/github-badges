{
  "version": 2,
  "builds": [
    { "src": "api/*.ts", "use": "@vercel/node" },
    { "src": "index.ts", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/ghrvc", "dest": "/api/ghrvc.ts" },
    { "src": "/ghpvc", "dest": "/api/ghpvc.ts" },
    { "src": "/ghflwcnt", "dest": "/api/ghflwcnt.ts" },
    { "src": "/ghfc", "dest": "/api/ghfc.ts" },
    { "src": "/ghsc", "dest": "/api/ghsc.ts" },
    { "src": "/", "dest": "/index.ts" }
  ]
}
