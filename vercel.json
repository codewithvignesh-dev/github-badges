{
  "version": 2,
  "builds": [
    { "src": "index.ts", "use": "@vercel/node" },
    { "src": "ghpvc.ts", "use": "@vercel/node" },
    { "src": "ghrvc.ts", "use": "@vercel/node" },
    { "src": "ghflwcnt.ts", "use": "@vercel/node" },
    { "src": "ghsc.ts", "use": "@vercel/node" },
    { "src": "ghfc.ts", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/", "dest": "/index.ts" },
    { "src": "/ghpvc", "dest": "/ghpvc.ts" },
    { "src": "/ghrvc", "dest": "/ghrvc.ts" },
    { "src": "/ghflwcnt", "dest": "/ghflwcnt.ts" },
    { "src": "/ghsc", "dest": "/ghsc.ts" },
    { "src": "/ghfc", "dest": "/ghfc.ts" }
  ]
}
